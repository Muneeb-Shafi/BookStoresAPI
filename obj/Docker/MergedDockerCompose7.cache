name: dockercompose18277504539902159078
services:
  bookstoresapi:
    build:
      context: C:\Users\Phant\source\repos\BookStoresAPI
      dockerfile: BookStoresAPI/Dockerfile
    depends_on:
      db:
        condition: service_started
        required: true
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
      ConnectionStrings__DefaultConnection: Host=db;Database=bookstoretest;Username=user;Password=password
    image: bookstoresapi
    networks:
      bookstore-network: null
    ports:
      - mode: ingress
        target: 80
        published: "5000"
        protocol: tcp
      - mode: ingress
        target: 80
        protocol: tcp
      - mode: ingress
        target: 443
        protocol: tcp
    volumes:
      - type: bind
        source: C:\Users\Phant\AppData\Roaming/Microsoft/UserSecrets
        target: /root/.microsoft/usersecrets
        read_only: true
        bind:
          create_host_path: true
      - type: bind
        source: C:\Users\Phant\AppData\Roaming/ASP.NET/Https
        target: /root/.aspnet/https
        read_only: true
        bind:
          create_host_path: true
  db:
    environment:
      POSTGRES_DB: bookstoreApi
      POSTGRES_PASSWORD: password
      POSTGRES_USER: user
    image: postgres
    networks:
      bookstore-network: null
    ports:
      - mode: ingress
        target: 5432
        published: "5432"
        protocol: tcp
    volumes:
      - type: volume
        source: postgres-data
        target: /var/lib/postgresql/data
        volume: {}
networks:
  bookstore-network:
    name: dockercompose18277504539902159078_bookstore-network
volumes:
  postgres-data:
    name: dockercompose18277504539902159078_postgres-data